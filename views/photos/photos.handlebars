{{> header}}

<div class="container">
    <div class="row mt-3 mx-auto">
        {{#each photos}}
        <div class="card mr-2" style="width: 10rem;">
            <a href="/photos/{{this._id}}"><img src="{{this.image}}" alt=""  class="img-thumbnail mt-3 d-inline-block"></a>
        </div>
        {{/each}}
        <div class="col-md-6 col-sm-4"><a href="/photos/new"><ion-icon name="add-circle" class="text-primary mt-5" ></ion-icon></a></div>
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <footer class="footer-dash">
                <a href="/logout"><ion-icon name="exit" class="logout"></ion-icon></a>
            </footer>
        </div>
    </div>
</div>

{{!-- {{> footer}} --}}